{
    "fmt.Errorf": {
        "prefix": "fe",
        "body": "fmt.Errorf(\"${1:failed to} ${2:action}: %w\", ${3:err})$0",
        "description": "Snippet for fmt.Errorf()"
    },
    "slogerr": {
        "prefix": "slogerr",
        "body": "logger.Error(\"${1:msg}\", slog.Any(\"error\", ${2:err}))$0",
        "description": "Snippet for slog.Error"
    },
